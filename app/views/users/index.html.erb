<h1>ユーザー一覧</h1>
<h1>Users</h1>
 
 <table>
    <thead>
 	  <tr>
 	    <th>image</th>
        <th>name</th>
 	  </tr>
    </thead>
 		<tbody>
 		   <% @users.each do |user| %>
 		<tr>
 			<td><%= attachment_image_tag @user, :profile_image, format: 'jpeg', size: "100x100", fallback: "no_image.jpg" %></td>
 			<td><%= user.name %></td>
 			<!-- リンク先変更必要 -->
 			<td><%= link_to "Show",user_path(user.id)%>
 			</td>
 		</tr>
 		    <% end %>
 	    </tbody>
 </table>
